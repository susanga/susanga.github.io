---
layout: null
---
{
  "audios": [
  {% assign posts = (site.posts | where: "pageType" , "audio") %}
   {% for post in posts %}
      {
          "title": "{{ post.title }}",
          "href": "{{ post.url | prepend: site.baseurl | prepend: site.url }}", 
          "type": "{{ post.pageType }}",
          "thumbnailSmall": "{{ post.thumbnailSmall }}",
          "downloadLink": "{{ post.downloadLink }}"
      }{% if forloop.last  %}{% else %},{% endif %}
  {% endfor %}
  ]
}
