layout: null
---
{
  "audios": [
  {% assign posts = (site.posts | where: "pageType" , "audio") %}
   {% for post in posts %}
      {
          "title": "{{ post.title }}",
          "href": "{{ post.url }}", 
          "type": "{{ post.pageType }}"
      }{% if forloop.last  %}{% else %},{% endif %}
  {% endfor %}
  ]
}
