---
layout: null
---
{
  "featuredVideos": [
  {% assign posts = site.data.featured-videos limit: 5 %}
  {% assign count = 1 %}
   {% for post in posts %}
      {
          "id": "{{ forloop.index }}",
          "title": "{{ post.title }}",          
          "summary": "{{ post.summary }}",
          "thumbnail": "{{ post.thumbnail }}"
      }{% assign count = count + 1 %}{% if forloop.last  %}{% else %},{% endif %}
  {% endfor %}
  ]
}
