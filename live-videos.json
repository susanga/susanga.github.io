---
layout: null
---
{
  "videos": [
  {% for post in site.posts %}
    {% if post.pageType != null and post.pageType != empty and post.pageType contains 'video' %}           
      {
          "title": "{{ post.title }}",
          "href": "{{ post.url }}", 
          "type": "{{ post.pageType }}"
      }{% if forloop.last  %}{% else %},{% endif %}
  {% endif %}
  {% endfor %}
  ]
}
